programa {
	funcao inicio() {
	    inteiro n1
	  
	    escreva("Digite o primeiro numero:")
		leia(n1)
	
	    se(n1>20){
	        escreva(n1+" Maior que 20")
	    }
	    senao{
	        se(n1==20){
	            escreva(n1+" Igual a 20")
	        }
	        senao{
	            escreva(n1+" Menor que 20")
	        }
	    }
		
	}
}