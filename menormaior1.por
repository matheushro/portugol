programa {
	funcao inicio() {
	    inteiro n1
	    inteiro n2
	    
	    escreva("Digite o primeiro numero:")
		leia(n1)
		escreva("Digite o segundo numero:")
		leia(n2)
	
	    se(n1>n2){
	        escreva(n1+" Maior que "+n2)
	    }
	    senao{
	            escreva(n1+" Menor que "+n2)
	    }
		
	}
}